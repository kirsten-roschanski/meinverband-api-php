# MeinVerband API Configuration
# Copy this file to .env.local and update with your actual credentials

# API Base URL  
MEINVERBAND_API_URL=https://landesverband.meinverband.online/api_server.php/api

# API Authentication
MEINVERBAND_API_USER=verein-xxx
MEINVERBAND_API_PASSWORD=your_api_password

# Optional: API Timeout (in seconds)
MEINVERBAND_API_TIMEOUT=30

# Optional: SSL Verification (true/false)
MEINVERBAND_API_VERIFY_SSL=true